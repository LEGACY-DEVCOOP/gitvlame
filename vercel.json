{
  "version": 2,
  "builds": [
    {
      "src": "app/main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app/main.py"
    }
  ],
  "devCommand": "uvicorn app.main:app --reload",
  "installCommand": "pip install -r requirements.txt && prisma generate"
}
